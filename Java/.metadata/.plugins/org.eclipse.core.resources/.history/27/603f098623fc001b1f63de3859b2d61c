package abstrac;

public class Parrot extends Bird {

	public Parrot(String name) {
		super(name);
	}

	@Override
	public void eat() {
		// TODO Auto-generated method stub
		super.eat();
	}

	@Override
	public void breath() {
		// TODO Auto-generated method stub
		super.breath();
	}

	@Override
	public void fly() {
		System.out.println(getName()+ "flying");

		
	}

}
