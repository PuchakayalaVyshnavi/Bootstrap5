package oop1;

public class Car {
private String model;
private int color;

public void setModel(String model) {
	String validModel= model.toLowerCase();
	if(validModel.equals("ford")||validModel.equals("alto")) {
	this.model = model;
	}
	else {
		this.model="Unknown";
	}
}
public String getModel() {
	return this.model;
}

}
